{
  "name": "@guebbit/vue-library",
  "version": "1.0.0",
  "license": "AGPLv3.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "_dev": "npm run docs:dev",
    "build": "run-p type-check-only \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check-only": "vue-tsc --build --force",
    "preview": "vite preview",
    "lint": "eslint",
    "test:unit": "vitest",
    "test:e2e": "nightwatch tests/e2e/*",
    "test:kill": "cmd \"/C TASKKILL /IM node.exe /F\"",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs --port 8080",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "prepublish": "npm run lint && npm run build && npm run docs:build",
    "publish:public": "npm publish --access public"
  },
  "dependencies": {
    "@guebbit/css-toolkit": "file:../css-toolkit",
    "@guebbit/css-ui": "file:../css-ui",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@nightwatch/vue": "^3.1.1",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "chromedriver": "^129.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^8.1.1",
    "eslint-plugin-vue": "^9.23.0",
    "geckodriver": "^4.4.1",
    "jsdom": "^25.0.0",
    "nightwatch": "^3.6.3",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "sass-embedded": "^1.78.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.3.1",
    "vite-plugin-nightwatch": "^0.4.6",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vitepress": "^1.3.4",
    "vitest": "^2.1.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.21"
  }
}
