{
  "name": "@guebbit/vue-library",
  "version": "1.0.0",
  "license": "AGPLv3.0",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run docs:dev",
    "dev:wb": "vite --config vite.workbench.config.ts",
    "build": "run-p type-check-only \"build-only {@}\" --",
    "build-only": "vite build",
    "type-check-only": "vue-tsc --build --force",
    "preview": "vite preview",
    "lint": "eslint",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "vitest run",
    "test:unit:target": "vitest run tests/unit/Media.spec.ts",
    "test:e2e": "nightwatch --env chrome",
    "test:e2e:target": "nightwatch --continue-on-fail --test tests/e2e/Media.spec.ts --env chrome",
    "test:kill": "cmd \"/C TASKKILL /IM node.exe /F\"",
    "format": "prettier --write src/",
    "docs:dev": "vitepress dev docs --debug --port 8080",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs --port 8080",
    "prepublish": "npm run lint && npm run test:unit && npm run build && npm run docs:build",
    "publish:public": "npm publish --access public",
    "clean_cache": "rm -rf node_modules/.vite && rm -rf docs/.vitepress/cache && rm -rf docs/.vitepress/dist"
  },
  "dependencies": {
    "@guebbit/css-toolkit": "^1.1.3",
    "@guebbit/css-ui": "file:../css-ui",
    "color": "^4.2.3",
    "dotenv": "^16.4.5",
    "vue": "^3.4.29"
  },
  "devDependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@nightwatch/vue": "^3.1.2",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.7.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "chromedriver": "^129.0.2",
    "eslint": "^8.57.1",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-vue": "^9.28.0",
    "geckodriver": "^4.5.0",
    "jsdom": "^25.0.1",
    "nightwatch": "^3.8.0",
    "npm-run-all2": "^6.2.3",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "postcss-prefix-selector": "^2.0.0",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.79.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vite-plugin-nightwatch": "^0.4.6",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vitepress": "^1.4.0",
    "vitest": "^2.1.2",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
